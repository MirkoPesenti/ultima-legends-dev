<div class="form-group">
	<label for="levelUp.attribute-dex">
		Destrezza (DES) - 
		{{actor.system.attributes.dex.base}}
		{{#unless (eq actor.system.attributes.dex.base actorAttributesUpgraded.dex)}}
			<i class="fa fa-arrow-right"></i>
			{{actorAttributesUpgraded.dex}}
		{{/unless}}
	</label>
	<input
		type="radio"
		name="levelUp.attribute"
		id="levelUp.attribute-dex"
		value="dex"
		{{#if (eq actor.system.attributes.dex.base actorAttributesUpgraded.dex)}}disabled{{/if}}
		{{#if (eq formData.levelUp.attribute "dex")}}checked{{/if}}
	/>
</div>

